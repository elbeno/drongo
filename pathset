#!/bin/sh
MUDROOT=/home/bjd1000/drongo
MUDDIR=$MUDROOT/main/fridge
MUDBOOTFILE=$MUDROOT/boot/boot.u
MUDINCLUDEDIR=$MUDROOT/boot
MUDLOGFILE=$MUDROOT/main/fridge/um.log
MUDSERVERHOST=$MUDROOT/main/fridge/serverhost
MUDUTILDIR=$MUDROOT/util

export MUDROOT MUDDIR MUDBOOTFILE MUDINCLUDEDIR
export MUDLOGFILE MUDSERVERHOST MUDUTILDIR

